{
  "version": 2,
  "builds": [
    { "src": "api/**/handler.go", "use": "@vercel/go" },
    { "src": "src/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api", "dest": "/api/api/handler.go" },
    { "src": "/search", "dest": "/api/search/handler.go" },
    { "src": "/(.*)", "dest": "/src/$1" }
  ]
}

